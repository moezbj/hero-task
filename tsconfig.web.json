{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/src/env.d.ts",
    "src/renderer/src/**/**/*",
    "src/renderer/src/**/*.tsx",
    "src/preload/*.d.ts"
  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": "./src/renderer/src",
    "paths": {
      "@components/*": [
        "app/components/*"
      ],
      "@requests/*": [
        "requests/*"
      ],
      "@config/*": [
        "config/*"
      ]
    }
  }
}
